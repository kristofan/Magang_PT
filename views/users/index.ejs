<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users</title>
  <link rel="stylesheet"  type="text/css" href="/index.css">
</head>
<body>
  <h1>Users List</h1>
<ul class="users-list">
  <% users.forEach(user => { %>
    <li class="user-item">
      <div class="user-info">
        <span class="user-name"><%= user.name %></span>
        <span class="user-email"><%= user.email %></span>
      </div>
    </li>
  <% }) %>
</ul>

</body>
</html>
